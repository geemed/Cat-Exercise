{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
      "indent": ["error", 4],
      "quotes": ["error", "single"],
      "no-console": [0]
    }
}
